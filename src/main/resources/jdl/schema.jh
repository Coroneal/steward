entity History {
    entityId Long required,
	date ZonedDateTime required,
    eventType EventType,
    eventEntity EventEntity
}

enum EventType {
    DEAL, ACCEPT_DEAL, PAYMENT
}

enum EventEntity {
    MECHANIC, USER
}
